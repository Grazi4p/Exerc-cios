#include <stdio.h>

int main() {
    // Inicializa a variÃ¡vel de controle com 10
    int i = 10;

    // Enquanto i for maior ou igual a 1, imprime o valor de i e decrementa
    while (i >= 1) {
        printf("%d ", i);
        i--;
    }

    return 0;
}
