#include <stdio.h>

int main() {
    int i = 1; // Inicializa a vari√°vel de controle

    while (i <= 10) { // Enquanto i for menor ou igual a 10
        printf("%d ", i); // Imprime o valor de i
        i++; // Incrementa i
    }

    return 0;
}